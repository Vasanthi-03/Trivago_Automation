<!--DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd"-->
<suite name="TrivagoHotelSuite" parallel="tests" thread-count="2">
    <test name="ChromeTest">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.trivago.tests.TrivagoTest"/>
        </classes>
    </test>

    <test name="EdgeTest">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="com.trivago.tests.TrivagoTest"/>
        </classes>
    </test>
</suite>

